defaultSpecies
(
    EMg 1.0
);

liquidPhaseName alphaSolid;
vaporPhaseName  alphaGas;

shapes
(
    ChannelSide1
    {
        shape    plane;
        
        planeCoeffs
        {
            point   (0 0 0);
            normal   (-1 0.1 0);
        }
        
        delVapor 0.0;
        U        (0 0 0);
        
        liquidSpecies
        (
            EMg 1.0
        );
        
        vaporSpecies
        (
        );
    }
    
    ChannelSide2
    {
        shape    plane;
        
        planeCoeffs
        {
            point   (-0.02999 0 0);
            normal   (1 -0.1 0);
        }
        
        delVapor 0.0;
        U        (0 0 0);
        
        liquidSpecies
        (
            EMg 1.0
        );
        
        vaporSpecies
        (
        );
    }
);


