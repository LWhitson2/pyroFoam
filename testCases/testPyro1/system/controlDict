FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pyroFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         3e-5;

deltaT          1e-5;

writeControl    timeStep; //adjustableRunTime;

writeInterval   1; //1e-2;

purgeWrite      0;

writeFormat     binary;

writePrecision  10;

writeCompression on;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  no;

maxCo           0.4;


